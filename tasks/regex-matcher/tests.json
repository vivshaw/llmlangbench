{
  "tests": [
    {
      "name": "exact literal match",
      "input": "abc\nabc",
      "expected": "true"
    },
    {
      "name": "literal mismatch",
      "input": "abc\ndef",
      "expected": "false"
    },
    {
      "name": "literal must be full match",
      "input": "a\nab",
      "expected": "false"
    },
    {
      "name": "dot matches any char",
      "input": "a.c\naxc",
      "expected": "true"
    },
    {
      "name": "dot does not match empty",
      "input": "a.c\nac",
      "expected": "false"
    },
    {
      "name": "star zero occurrences",
      "input": "ab*c\nac",
      "expected": "true"
    },
    {
      "name": "star multiple occurrences",
      "input": "ab*c\nabbbc",
      "expected": "true"
    },
    {
      "name": "star matches empty string",
      "input": "a*\n",
      "expected": "true"
    },
    {
      "name": "plus requires at least one",
      "input": "ab+c\nac",
      "expected": "false"
    },
    {
      "name": "plus matches one or more",
      "input": "ab+c\nabbc",
      "expected": "true"
    },
    {
      "name": "question mark zero occurrences",
      "input": "ab?c\nac",
      "expected": "true"
    },
    {
      "name": "question mark one occurrence",
      "input": "ab?c\nabc",
      "expected": "true"
    },
    {
      "name": "question mark rejects two",
      "input": "ab?c\nabbc",
      "expected": "false"
    },
    {
      "name": "alternation first branch",
      "input": "cat|dog\ncat",
      "expected": "true"
    },
    {
      "name": "alternation second branch",
      "input": "cat|dog\ndog",
      "expected": "true"
    },
    {
      "name": "alternation no match",
      "input": "cat|dog\nrat",
      "expected": "false"
    },
    {
      "name": "simple group with quantifier",
      "input": "(ab)+\nabab",
      "expected": "true"
    },
    {
      "name": "group star matches empty",
      "input": "(ab)*\n",
      "expected": "true"
    },
    {
      "name": "group star matches multiple",
      "input": "(ab)*\nababab",
      "expected": "true"
    },
    {
      "name": "character class",
      "input": "[aeiou]\ne",
      "expected": "true"
    },
    {
      "name": "character class mismatch",
      "input": "[aeiou]\nx",
      "expected": "false"
    },
    {
      "name": "character range",
      "input": "[a-z]+\nhello",
      "expected": "true"
    },
    {
      "name": "character range mismatch",
      "input": "[a-z]+\nhello123",
      "expected": "false"
    },
    {
      "name": "negated character class",
      "input": "[^0-9]+\nabc",
      "expected": "true"
    },
    {
      "name": "negated character class mismatch",
      "input": "[^0-9]+\nabc123",
      "expected": "false"
    },
    {
      "name": "escaped dot matches literal dot",
      "input": "a\\.b\na.b",
      "expected": "true"
    },
    {
      "name": "escaped dot rejects non-dot",
      "input": "a\\.b\naxb",
      "expected": "false"
    },
    {
      "name": "nested groups with alternation",
      "input": "((a|b)c)+\nacbc",
      "expected": "true"
    },
    {
      "name": "nested groups no match",
      "input": "((a|b)c)+\nabc",
      "expected": "false"
    },
    {
      "name": "complex pattern",
      "input": "[A-Z][a-z]+ [A-Z][a-z]+\nHello World",
      "expected": "true"
    },
    {
      "name": "dot star is greedy but backtracks",
      "input": "a.*b\naxxb",
      "expected": "true"
    },
    {
      "name": "multiple alternations",
      "input": "a|b|c\nb",
      "expected": "true"
    },
    {
      "name": "quantifier on character class",
      "input": "[abc]*\nabccba",
      "expected": "true"
    },
    {
      "name": "empty pattern matches empty string",
      "input": "\n",
      "expected": "true"
    },
    {
      "name": "alternation with groups and quantifiers",
      "input": "(foo|bar)+baz\nfoofoobarbaz",
      "expected": "true"
    },
    {
      "name": "greedy star must backtrack",
      "input": "a*a\naaa",
      "expected": "true"
    },
    {
      "name": "alternation requires backtracking",
      "input": "(a|ab)c\nabc",
      "expected": "true"
    },
    {
      "name": "dot in character class is literal",
      "input": "[.]\n.",
      "expected": "true"
    },
    {
      "name": "dot in character class does not match other chars",
      "input": "[.]\na",
      "expected": "false"
    },
    {
      "name": "multiple escaped metacharacters",
      "input": "\\.\\*\\+\n.*+",
      "expected": "true"
    },
    {
      "name": "escaped pipe is literal",
      "input": "\\|\n|",
      "expected": "true"
    },
    {
      "name": "escaped parentheses are literal",
      "input": "\\(a\\)\n(a)",
      "expected": "true"
    },
    {
      "name": "multiple ranges in character class",
      "input": "[a-zA-Z0-9]+\nhelloWorld123",
      "expected": "true"
    },
    {
      "name": "hyphen at start of class is literal",
      "input": "[-a]\n-",
      "expected": "true"
    },
    {
      "name": "hyphen at end of class is literal",
      "input": "[a-]\n-",
      "expected": "true"
    },
    {
      "name": "greedy backtracking with multiple dot-star",
      "input": "a.*b.*c\naXbYc",
      "expected": "true"
    },
    {
      "name": "dot-star cannot rearrange characters",
      "input": "a.*b.*c\naXcYb",
      "expected": "false"
    },
    {
      "name": "exact repetition",
      "input": "a{3}\naaa",
      "expected": "true"
    },
    {
      "name": "exact repetition - too few",
      "input": "a{3}\naa",
      "expected": "false"
    },
    {
      "name": "exact repetition - too many",
      "input": "a{3}\naaaa",
      "expected": "false"
    },
    {
      "name": "range repetition",
      "input": "a{2,4}\naaa",
      "expected": "true"
    },
    {
      "name": "range repetition - minimum",
      "input": "a{2,4}\naa",
      "expected": "true"
    },
    {
      "name": "range repetition - maximum",
      "input": "a{2,4}\naaaa",
      "expected": "true"
    },
    {
      "name": "range repetition - too few",
      "input": "a{2,4}\na",
      "expected": "false"
    },
    {
      "name": "range repetition - too many",
      "input": "a{2,4}\naaaaa",
      "expected": "false"
    },
    {
      "name": "open range repetition",
      "input": "a{2,}\naaaaa",
      "expected": "true"
    },
    {
      "name": "open range repetition - at minimum",
      "input": "a{2,}\naa",
      "expected": "true"
    },
    {
      "name": "open range repetition - too few",
      "input": "a{2,}\na",
      "expected": "false"
    },
    {
      "name": "counted repetition on group",
      "input": "(ab){3}\nababab",
      "expected": "true"
    },
    {
      "name": "counted repetition on group - wrong count",
      "input": "(ab){3}\nabab",
      "expected": "false"
    },
    {
      "name": "counted repetition with backtracking",
      "input": "a{2,4}a\naaa",
      "expected": "true"
    },
    {
      "name": "digit shorthand",
      "input": "\\d+\n12345",
      "expected": "true"
    },
    {
      "name": "digit shorthand rejects letters",
      "input": "\\d+\n123abc",
      "expected": "false"
    },
    {
      "name": "word shorthand",
      "input": "\\w+\nhello_World_42",
      "expected": "true"
    },
    {
      "name": "word shorthand rejects special chars",
      "input": "\\w+\nhello world",
      "expected": "false"
    },
    {
      "name": "whitespace shorthand",
      "input": "\\s+\n \t\n",
      "expected": "true"
    },
    {
      "name": "whitespace shorthand rejects non-whitespace",
      "input": "\\s+\n abc",
      "expected": "false"
    },
    {
      "name": "non-digit shorthand",
      "input": "\\D+\nabc",
      "expected": "true"
    },
    {
      "name": "non-digit shorthand rejects digits",
      "input": "\\D+\nabc123",
      "expected": "false"
    },
    {
      "name": "non-word shorthand",
      "input": "\\W+\n!@# ",
      "expected": "true"
    },
    {
      "name": "non-whitespace shorthand",
      "input": "\\S+\nhello",
      "expected": "true"
    },
    {
      "name": "mixed shorthands in pattern",
      "input": "\\w+@\\w+\\.\\w+\nuser@host.com",
      "expected": "true"
    },
    {
      "name": "shorthand with counted repetition",
      "input": "\\d{3}-\\d{4}\n555-1234",
      "expected": "true"
    },
    {
      "name": "shorthand with counted repetition - wrong format",
      "input": "\\d{3}-\\d{4}\n55-1234",
      "expected": "false"
    },
    {
      "name": "counted repetition on character class",
      "input": "[a-z]{5}\nhello",
      "expected": "true"
    },
    {
      "name": "complex pattern with all features",
      "input": "\\w{2,}@\\w+\\.(com|org|net)\nab@test.org",
      "expected": "true"
    },
    {
      "name": "performance - ReDoS optional quantifier",
      "input": "(a?){25}a{25}\naaaaaaaaaaaaaaaaaaaaaaaaa",
      "expected": "true"
    },
    {
      "name": "performance - nested quantifier no match",
      "input": "(a+)+b\naaaaaaaaaaaaaaaaaaaaaaaaa",
      "expected": "false"
    },
    {
      "name": "performance - alternation explosion no match",
      "input": "(a|a)*b\naaaaaaaaaaaaaaaaaaaaaaaaa",
      "expected": "false"
    }
  ]
}
