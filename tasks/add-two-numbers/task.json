{
  "id": "add-two-numbers",
  "spec": "spec.md",
  "tests": "tests.json",
  "languages": {
    "typescript": {
      "runCommand": "npx tsx run.ts",
      "setupCommand": "npm install"
    },
    "python": {
      "runCommand": "python3 run.py"
    },
    "rust": {
      "runCommand": "./target/release/add-two-numbers",
      "setupCommand": "cargo build --release"
    },
    "go": {
      "runCommand": "go run ./cmd",
      "setupCommand": "go build ./..."
    },
    "haskell": {
      "runCommand": "stack exec add-two-numbers-exe",
      "setupCommand": "stack build"
    },
    "java": {
      "runCommand": "./gradlew -q run",
      "setupCommand": "./gradlew build -x test"
    }
  }
}
