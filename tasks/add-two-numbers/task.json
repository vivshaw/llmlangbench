{
  "id": "add-two-numbers",
  "spec": "spec.md",
  "tests": "tests.json",
  "languages": {
    "typescript": {
      "runCommand": "npx tsx run.ts",
      "installCommand": "npm install",
      "testCommand": "npx vitest run",
      "testFramework": "Vitest"
    },
    "python": {
      "runCommand": "python3 run.py",
      "installCommand": "python3 -m venv .venv && .venv/bin/pip install pytest",
      "testCommand": ".venv/bin/python -m pytest",
      "testFramework": "pytest"
    },
    "rust": {
      "runCommand": "./target/release/add-two-numbers",
      "setupCommand": "cargo build --release",
      "testCommand": "cargo test",
      "testFramework": "cargo test (built-in #[test])"
    },
    "go": {
      "runCommand": "go run ./cmd",
      "testCommand": "go test -v ./...",
      "testFramework": "go test (testing package)"
    },
    "haskell": {
      "runCommand": "stack exec add-two-numbers-exe",
      "setupCommand": "stack build",
      "testCommand": "stack test",
      "testFramework": "HSpec"
    },
    "java": {
      "runCommand": "./gradlew -q run",
      "setupCommand": "./gradlew build -x test",
      "testCommand": "./gradlew test",
      "testFramework": "JUnit 5"
    }
  }
}
